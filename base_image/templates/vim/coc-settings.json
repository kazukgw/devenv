{
  "list.height": 30,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "typescript",
    "javascript",
    "go",
    "css",
    "markdown",
    "json"
  ],
  "python.jediEnabled": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "disableWorkspaceFolders": true,
      "filetypes": [
        "go"
      ]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": [
        "terraform"
      ],
      "initializationOptions": {}
    }
  }
}
